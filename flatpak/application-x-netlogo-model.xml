  <mime-type type="application/x-netlogo-model">
    <comment>NetLogo Model</comment>
    <comment xml:lang="en">NetLogo Model</comment>
    <glob pattern="*.nlogo"/>
    <glob pattern="*.nlogo3d"/>
    <glob pattern="*.nlogox"/>
